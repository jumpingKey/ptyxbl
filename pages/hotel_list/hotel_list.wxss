.container {
  background-color: #f2f2f2;
}

.item {
  padding: 8px 0px 8px 0px;
  width: 100%;
  background-color: #fff;
  color: #787878;
  border-top: 1px solid #eaeaea;
  border-bottom: 1px solid #eaeaea;
  align-items: center;
  justify-content: space-between;
}

.item image {
  display: block;
  width: 72px;
  height: 72px;
  padding-left: 8px;
}

.info {
  flex: 1;
  margin-left: 10px;
  overflow: hidden;
}

.name {
  display: block;
  font-size: 14px;
  color: #333;
  margin-bottom: 2px;
}

.score {
  font-size: 12px;
  color: #49f;
  margin: 2px 0px 2px;
  font-weight: bold;
}

.address {
  display: block;
  font-size: 12px;
  color: #666;
  margin: 2px 0px;
}

text {
  font-size: 12px;
}

.right_wrap .pri {
  margin-right: 20px;
  font-size: 16px;
  font-weight: bold;
  color: #f55;
}

.c666 {
  font-size: 11px;
}

.facility {
  display: inline-block;
}

.facility image {
  display: inline-block;
  margin: 2px 0px -3px;
  height: 16px;
  width: 16px;
  padding-left: 5px;
  
}
